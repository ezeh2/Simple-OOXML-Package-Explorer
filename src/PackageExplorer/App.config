<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	
	<configSections>
		<section name="packageExplorer" type="PackageExplorer.Configuration.PackageExplorerSection, PackageExplorer" />
	</configSections>
	
	<packageExplorer defaultViewer="XmlViewer">
		
		<packageTypes>
			<add contentType="http://schemas.openxmlformats.org/officeDocument/2006/relationships/package" />
		</packageTypes>
		
		<partViewers>
			<add name="ZipImageViewer" contentTypes="image/x-emz" type="PackageExplorer.View.ZipImagePartViewer, PackageExplorer" />
			<add name="ImageViewer" contentTypes="image/png|image/jpeg|image/gif" type="PackageExplorer.View.ImagePartViewer, PackageExplorer" />
			<add name="XmlViewer" contentTypes="text/xml" type="PackageExplorer.View.XmlPartViewer, PackageExplorer" />
		</partViewers>
	</packageExplorer>
</configuration>